{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/Images/*",
      "/images/*",
      "/*.{css,js,png,jpg,gif,ico,svg,jpeg,avif,woff,woff2,ttf,eot,json,map}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}

