{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/Images/*",
      "/images/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.ico",
      "/*.svg",
      "/*.avif",
      "/*.webp",
      "/*.woff",
      "/*.woff2",
      "/*.ttf",
      "/*.eot",
      "/*.json",
      "/*.map"
    ]
  },
  "routes": [
    {
      "route": "/Images/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}

